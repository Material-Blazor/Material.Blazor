@namespace BlazorMdc.Demo

<MTTextField Value="@StringValue"
                ValueChanged="@StringValueChanged"
                ValueExpression="@( () => StringValue )"
                Label="@Label"
                LeadingIcon="@LeadingIcon"
                @onfocus="@OnFocus"
                TrailingIcon="@TrailingIcon"
                type="@Type" />

@code{
#nullable enable annotations

    [Parameter] public string Label { get; set; } = "";
    [Parameter] public string? LeadingIcon { get; set; }
    [Parameter] public EventCallback<FocusEventArgs> OnFocus { get; set; }
    [Parameter] public string? TrailingIcon { get; set; }
    [Parameter] public string Type { get; set; } = "text";
    [Parameter] public string StringValue { get; set; }
    [Parameter] public EventCallback<string> StringValueChanged { get; set; }
    [Parameter] public Expression<Func<string>> pStringValueExpression { get; set; }

#nullable restore annotations
}
