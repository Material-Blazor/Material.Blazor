{
  "metadata": [
    {
      "src": [
        {
          "files": "Material.Blazor.csproj",
          "filter": "filterConfig.yml"
        }
      ],
      "allowCompilationErrors": true,
      "dest": "api",
      "disableDefaultFilter": false,
      "disableGitFeatures": false,
      "includePrivateMembers": false,
      "namespaceLayout": "flattened",
      "noRestore": false,
      "memberLayout": "samePage"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**/**.md",
          "articles/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "Material.Blazor/**/**.md",
          "Material.Blazor/**/**/**.md",
          "Material.Blazor/**/toc.yml"
        ],
        "exclude": [
          "Material.Blazor/node_modules/**/**.md",
          "Material.Blazor/node_modules/**/**.toc"
        ]
      }

    ],
    "resource": [
      {
        "files": [
          "Material.Blazor/**/images/**.png",
          "Material.Blazor/**/images/**.jpg",
          "Material.Blazor/.nojekyll",
          "Material.Blazor/404.html",
          "Material.Blazor/*html.txt"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api/**.md"
        ]
      }
    ],
    "output": "siteDocFx",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "allowcompilationerrors": true,
    "cleanupCacheHistory": false,
    "disableContribution": true,
    "disableGitFeatures": false,
    "keepFileLink": false,
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "postProcessors": [],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}