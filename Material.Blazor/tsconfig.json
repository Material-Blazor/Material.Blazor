    "allowJs": true,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "outFile": "typescript.js",
    "removeComments": false,
    "sourceMap": false,
    "target": "es5"
  },
  "include": [ "Scripts/*.ts" ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
