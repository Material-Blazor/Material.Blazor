{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "outDir": ".",
    "removeComments": false,
    "sourceMap": false,
    "target": "es5"
  },
  "include": [ "Scripts/*.ts" ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
