{
  "name": "material.blazor",
  "version": "2.0.0",
  "description": "NPM build steps for Material.Blazor",
  "repository": "https://github.com/Material-Blazor/Material.Blazor",
  "scripts": {
    "build-css": "sass --no-color --unicode --style=expanded --load-path=node_modules --no-source-map Styles/MaterialBlazor.scss wwwroot/MaterialBlazor.css",
    "build-min-css": "sass --no-color --unicode --style=compressed --load-path=node_modules --no-source-map Styles/MaterialBlazor.scss wwwroot/MaterialBlazor.min.css",
    "build-material-css": "sass --no-color --unicode --style=expanded --load-path=node_modules --no-source-map Styles/Material.scss wwwroot/Material.css",
    "build-material-min-css": "sass --no-color --unicode --style=compressed --load-path=node_modules --no-source-map Styles/Material.scss wwwroot/Material.min.css",
    "build-js": "terser wwwroot/intermediate.js --beautify --output wwwroot/MaterialBlazor.js",
    "build-min-js": "terser wwwroot/MaterialBlazor.js --compress --mangle --ecma 2016 --output wwwroot/MaterialBlazor.min.js",
    "build-scripts": "webpack --config webpack.config.js",
    "build-scripts-Comment1": "https://github.com/microsoft/TypeScript-Babel-Starter",
    "build-scripts-Comment2": "https://github.com/a-tarasyuk/webpack-typescript-babel"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "7.14.7",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-typescript": "7.14.5",
    "babel-loader": "8.2.2",
    "fork-ts-checker-webpack-plugin": "6.2.12",
    "lodash.debounce": "4.0.8",
    "lodash.throttle": "4.1.1",
    "material-components-web": "^9.0.0",
    "regexp": "^1.0.0",
    "sass": "1.35.2",
    "terser": "5.7.1",
    "ts-loader": "9.2.3",
    "typescript": "4.3.5",
    "webpack": "5.45.1",
    "webpack-cli": "4.7.2"
  }
}
