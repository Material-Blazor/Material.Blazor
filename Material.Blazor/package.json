{
  "name": "material.blazor",
  "version": "2.0.0",
  "description": "NPM build steps for Material.Blazor",
  "repository": "https://github.com/Material-Blazor/Material.Blazor",
  "scripts": {
    "buildStaticWebAssets": "npm run buildStaticWebAssetsCSS&&npm run buildStaticWebAssetsJS&&npm --version",
    "buildStaticWebAssetsCSS": "npm run build-css&&npm run build-min-css&&npm run build-material-css&&npm run build-material-min-css&&npm --version",
    "build-css": "sass --no-color --unicode --style=expanded --load-path=node_modules --no-source-map Styles/material.blazor.scss StaticAssets/material.blazor.css",
    "build-min-css": "sass --no-color --unicode --style=compressed --load-path=node_modules --no-source-map Styles/material.blazor.scss StaticAssets/material.blazor.min.css",
    "build-material-css": "sass --no-color --unicode --style=expanded --load-path=node_modules --no-source-map Styles/material-components-web.scss StaticAssets/material-components-web.css",
    "build-material-min-css": "sass --no-color --unicode --style=compressed --load-path=node_modules --no-source-map Styles/material-components-web.scss StaticAssets/material-components-web.min.css",
    "buildStaticWebAssetsJS": "npm run build-scripts&&npm run build-js&&npm run build-min-js&&del .\\StaticAssets\\intermediate.js&&npm --version",
    "build-js": "terser StaticAssets/intermediate.js --beautify --output StaticAssets/material.blazor.js",
    "build-min-js": "terser StaticAssets/material.blazor.js --compress --mangle --ecma 2016 --output StaticAssets/material.blazor.min.js",
    "build-scripts": "webpack --config webpack.config.js",
    "build-scripts-Comment1": "https://github.com/microsoft/TypeScript-Babel-Starter",
    "build-scripts-Comment2": "https://github.com/a-tarasyuk/webpack-typescript-babel"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "babel-loader": "^8.2.5",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "material-components-web": "14.0.0",
    "regexp": "^1.0.0",
    "remove-files-webpack-plugin": "^1.5.0",
    "sass": "1.39.2",
    "terser": "^5.14.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
