{
  "name": "material.blazor",
  "version": "1.0.0",
  "description": "NPM build steps for Material.Blazor",
  "repository": "https://github.com/Material-Blazor/Material.Blazor",
  "scripts": {
    "build-css": "dart-sass --update --no-color --unicode --style=expanded --load-path=node_modules --no-source-map Styles/unbundled.scss wwwroot/unbundled.css",
    "build-min-css": "dart-sass --update --no-color --unicode --style=compressed --load-path=node_modules --no-source-map Styles/unbundled.scss wwwroot/unbundled.min.css",
    "build-min-bundle-css": "dart-sass --update --no-color --unicode --style=compressed --load-path=node_modules --no-source-map Styles/bundled.scss wwwroot/bundled.min.css",
    "build-js": "terser wwwroot/MaterialBlazor.js --beautify --output wwwroot/unbundled.js",
    "build-min-js": "terser wwwroot/unbundled.js --compress --mangle --ecma 2016 --output wwwroot/unbundled.min.js",
    "build-min-bundle-js-Comment1": "unbundled.js must be after material-components-web otherwise the tooltips fail",
    "build-min-bundle-js": "terser node_modules/material-components-web/dist/material-components-web.js wwwroot/unbundled.js --compress --mangle --ecma 2016 --output wwwroot/bundled.min.js",
    "build-scripts": "webpack --config webpack.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "dart-sass": "^1.25.0",
    "material-components-web": "8.0.0",
    "regexp": "^1.0.0",
    "terser": "^4.8.0",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@material/base": "8.0.0",
    "@material/button": "8.0.0",
    "@material/card": "8.0.0",
    "@material/checkbox": "8.0.0",
    "@material/chips": "8.0.0",
    "@material/data-table": "8.0.0",
    "@material/dialog": "8.0.0",
    "@material/drawer": "8.0.0",
    "@material/elevation": "8.0.0",
    "@material/fab": "8.0.0",
    "@material/form-field": "8.0.0",
    "@material/icon-button": "8.0.0",
    "@material/layout-grid": "8.0.0",
    "@material/linear-progress": "8.0.0",
    "@material/list": "8.0.0",
    "@material/menu": "8.0.0",
    "@material/radio": "8.0.0",
    "@material/ripple": "8.0.0",
    "@material/select": "8.0.0",
    "@material/slider": "8.0.0",
    "@material/snackbar": "8.0.0",
    "@material/switch": "8.0.0",
    "@material/tab-bar": "8.0.0",
    "@material/tab-scroller": "8.0.0",
    "@material/textfield": "8.0.0",
    "@material/theme": "8.0.0",
    "@material/tooltip": "8.0.0",
    "@material/top-app-bar": "8.0.0",
    "@material/typography": "8.0.0"
  }
}
