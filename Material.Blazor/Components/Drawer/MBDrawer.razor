@namespace Material.Blazor
@inherits ComponentFoundation


<aside @ref="DrawerElem"
       @attributes="@AttributesToSplat()">

    <div class="mdc-drawer__content">
        <CascadingValue Value="@this">
            @if (ChildContent != null)
            {
                @ChildContent
            }
        </CascadingValue>
    </div>

</aside>