{
  "name": "blazor-mdc.node-material-components",
  "version": "0.0.0",
  "description": "BlazorMdc.NodeMaterialComponents",
  "main": "server.js",
  "scripts": {
    "clean-css": "rmdir /S /Q dist; rmdir /S /Q downloads",
    "copy": "xcopy /y dist\\*.css ..\\BlazorMdc\\wwwroot",
    "dev": "npm run development",
    "delete": "del /q dist\\blazormdc-bundled.css",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "download-icons-css": "powershell.exe -Command \"New-Item -Path '.' -Name 'downloads' -ItemType 'directory'; Invoke-WebRequest -Uri 'https://fonts.googleapis.com/css?family=Material+Icons|Material+Icons+Outlined|Material+Icons+Two+Tone|Material+Icons+Round|Material+Icons+Sharp' -OutFile '.\\downloads\\material-icons.css'\"",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "prod-rename-1": "rename dist\\blazormdc.css blazormdc.min.css",
    "prod-rename-2": "rename dist\\blazormdc-bundled.css blazormdc-bundled.min.css",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0",
    "css-loader": "^3.6.0",
    "dart-sass": "^1.25.0",
    "extract-loader": "^5.1.0",
    "laravel-mix": "^5.0.4",
    "material-components-web": "^7.0.0",
    "material-design-icons": "^3.0.1",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "vue-template-compiler": "^2.6.11"
  },
  "-vs-binding": {
    "BeforeBuild": [
      "install"
    ],
    "AfterBuild": [
      "clean-css",
      "download-icons-css",
      "prod",
      "prod-rename-1",
      "prod-rename-2",
      "dev",
      "delete",
      "copy"
    ]
  }
}
