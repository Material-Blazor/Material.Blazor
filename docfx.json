{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Material.Blazor/Material.Blazor.csproj"
          ],
          "filter": "filterConfig.yml"
        }
      ],
      "dest": "api",
      "disableContribution": true,
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "articles/**/**.md",
          "articles/toc.yml"
        ]
      },
      {
        "files": [
          "Material.Blazor/**/**.md",
          "Material.Blazor/**/**/**.md",
          "Material.Blazor/**/toc.yml"
        ],
        "exclude": [
          "Material.Blazor/node_modules/**/**.md",
          "Material.Blazor/node_modules/**/**.toc"
        ]
      },
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },

    ],
    "resource": [
      {
        "files": [
          "images/**.png",
          "images/**.jpg",
          ".nojekyll",
          "404.html",
          "*html.txt"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api/**.md"
        ],
        "exclude": [
          "obj/**",
          "siteDocFx/**"
        ]
      }
    ],
    "dest": "siteDocFx",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material.blazor"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "disableContribution": true,
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}