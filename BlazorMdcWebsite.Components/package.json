{
  "name": "blazor-mdc-website-node-build",
  "version": "0.0.0",
  "description": "Node build steps for BlazorMdcWebsite",
  "main": "server.js",
  "scripts": {
    "build-mt-default": "dart-sass --load-path=node_modules --no-source-map Styles/mt-default-theme.scss wwwroot/css/mt-default-theme.css",
    "minify-mt-default": "cleancss --level 1 --format breaksWith=lf --output wwwroot/css/mt-default-theme.min.css wwwroot/css/mt-default-theme.css",
    "build-blue-square": "dart-sass --load-path=node_modules --no-source-map Styles/blue-square-theme.scss wwwroot/css/blue-square-theme.css",
    "minify-blue-square": "cleancss --level 1 --format breaksWith=lf --output wwwroot/css/blue-square-theme.min.css wwwroot/css/blue-square-theme.css",
    "build-red-round": "dart-sass --load-path=node_modules --no-source-map Styles/red-round-theme.scss wwwroot/css/red-round-theme.css",
    "minify-red-round": "cleancss --level 1 --format breaksWith=lf --output wwwroot/css/red-round-theme.min.css wwwroot/css/red-round-theme.css",
    "build-varied": "dart-sass --load-path=node_modules --no-source-map Styles/varied-theme.scss wwwroot/css/varied-theme.css",
    "minify-varied": "cleancss --level 1 --format breaksWith=lf --output wwwroot/css/varied-theme.min.css wwwroot/css/varied-theme.css"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "clean-css-cli": "^4.3.0",
    "material-components-web": "7.0.0",
    "dart-sass": "^1.25.0",
    "terser": "^4.8.0",
    "typescript": "^3.9.7"
  }
}
