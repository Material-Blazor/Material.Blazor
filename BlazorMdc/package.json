{
  "name": "blazor-mdc-node-build",
  "version": "0.0.0",
  "description": "Node build steps for BlazorMdc",
  "main": "server.js",
  "scripts": {
    "build-bmdc-css": "dart-sass --load-path=node_modules --no-source-map Styles/blazormdc.scss wwwroot/blazormdc.css",
    "minify-bmdc-css": "cleancss --level 1 --format breaksWith=lf --output wwwroot/blazormdc.min.css wwwroot/blazormdc.css",
    "build-bundle-css": "dart-sass --load-path=node_modules --no-source-map Styles/blazormdc-material-bundle.scss wip/blazormdc-material-bundle.css",
    "minify-bundle-css": "cleancss --level 1 --format breaksWith=lf --output wwwroot/blazormdc-material-bundle.min.css wip/blazormdc-material-bundle.css",
    "build-bmdc-js": "terser Scripts/blazormdc.js node_modules/@material/tooltip/dist/mdc.tooltip.js --output wwwroot/blazormdc.js",
    "build-bmdc-ts-realbutcausesfailure": "tsc",
    "minify-bmdc-js": "terser wwwroot/blazormdc.js node_modules/@material/tooltip/dist/mdc.tooltip.js --compress --mangle --ecma 2016 --output wwwroot/blazormdc.min.js",
    "minify-bundle-js": "terser wwwroot/blazormdc.js node_modules/material-components-web/dist/material-components-web.js node_modules/@material/tooltip/dist/mdc.tooltip.js --compress --mangle --ecma 2016 --output wwwroot/blazormdc-material-bundle.min.js",
    "build-bmdcdemo": "dart-sass --load-path=node_modules --no-source-map ../BlazorMdc.Demo.CommonUI/Styles/styles.scss ../BlazorMdc.Demo.CommonUI/wwwroot/css/styles.css"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "clean-css-cli": "^4.3.0",
    "material-components-web": "7.0.0",
    "@material/tooltip": "8.0.0-canary.2e5711e04.0",
    "dart-sass": "^1.25.0",
    "terser": "^4.8.0",
    "typescript": "^3.9.7"
  }
}
