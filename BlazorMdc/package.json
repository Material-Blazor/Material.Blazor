{
  "name": "blazor-mdc-node-build",
  "version": "0.0.0",
  "description": "Node build steps for BlazorMdc",
  "main": "server.js",
  "scripts": {
    "build-css": "dart-sass --update --no-color --unicode --style=expanded --load-path=node_modules --no-source-map Styles/blazormdc.scss wwwroot/blazormdc.css",
    "build-min-css": "dart-sass --update --no-color --unicode --style=compressed --load-path=node_modules --no-source-map Styles/blazormdc.scss wwwroot/blazormdc.min.css",
    "build-min-bundle-css": "dart-sass --update --no-color --unicode --style=compressed --load-path=node_modules --no-source-map Styles/blazormdc-material-bundle.scss wip/blazormdc-material-bundle.min.css",
    "build-js": "terser Scripts/blazormdc.js node_modules/@material/tooltip/dist/mdc.tooltip.js --output wwwroot/blazormdc.js",
    "build-min-js": "terser wwwroot/blazormdc.js node_modules/@material/tooltip/dist/mdc.tooltip.js --compress --mangle --ecma 2016 --output wwwroot/blazormdc.min.js",
    "build-min-bundle-js": "terser wwwroot/blazormdc.js node_modules/material-components-web/dist/material-components-web.js node_modules/@material/tooltip/dist/mdc.tooltip.js --compress --mangle --ecma 2016 --output wwwroot/blazormdc-material-bundle.min.js",
    "build-ts-realbutcausesfailure": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "material-components-web": "7.0.0",
    "@material/tooltip": "8.0.0-canary.2e5711e04.0",
    "dart-sass": "^1.25.0",
    "terser": "^4.8.0",
    "typescript": "^3.9.7"
  }
}
